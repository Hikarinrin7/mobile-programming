//関数

int add(int a, int b) {
//   var num = a + b;
//   return num;
//と書きたいところだが
  return a + b;
}

//voidという型は戻り値がない
void showAdd(int hoge, int fuga) {
  print (hoge + fuga);
}

void main() {
  print(1 + 3);
//   var result = add(5, 10);
//   print(result);
//と書きたいところだが
  print(add(5, 10));
  
  showAdd(5,25);
  
}
