void main() {
  
  var price = 100;
  
  ///Boolean（ブーリアン）。trueとfalseがある、「ブール値」
  var bool1 = price > 500;
  var bool2 = price < 500;
  print("$bool1 $bool2");
  
  ///条件分岐
  ///if文の（）の中がfalseになるときは実行されない
  if (price > 500) {
    print("高い");
  } else if (price > 400) {
    print("安め");
  } else {
    print("安すぎる");
  }
  
  if (price < 500) {
    print(price);
  }
  
}
